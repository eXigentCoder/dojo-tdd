  "engines": {
    "node": ">=6.10.2"
  },
  "description": "Code dojo for teaching TDD",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/istanbul --include-all-sources cover -x \"public/**\" -x \"report/**\" -x \"utilities/**\" node_modules/mocha/bin/_mocha \"test/**/*.js\"",
    "lint": "node_modules/.bin/eslint \"src/**/*.js\" \"test/**/*.js\" index.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eXigentCoder/tdd-dojo.git"
  },
  "keywords": [
    "TDD",
    "code-dojo",
    "dojo",
    "node"
  ],
  "author": "Ryan Kotzen <potz666@gmail.com> (http://blog.exigentcoder.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eXigentCoder/tdd-dojo/issues"
  },
  "homepage": "https://github.com/eXigentCoder/tdd-dojo#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.19.0",
    "eslint-plugin-mocha": "4.9.0",
    "eslint-plugin-node": "4.2.2",
    "istanbul": "0.4.5",
    "mocha": "3.3.0",
    "node-mocks-http": "1.6.1",
    "proxyquire": "1.7.11",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "supertest": "3.0.0"
  },
  "dependencies": {
    "express": "4.15.2",
    "lodash": "4.17.4"
  }
}
